{
    "index_patterns":[
       "nextgisweb_audit-*"
    ],
    "mappings":{
       "properties":{
          "@timestamp":{
             "type":"date"
          },
          "request":{
             "type":"nested",
             "properties":{
                "method":{
                   "type":"keyword"
                },
                "path":{
                   "type":"keyword"
                }
             }
          },
          "response":{
             "type":"nested",
             "properties":{
                "status_code":{
                   "type":"keyword"
                },
                "route_name":{
                   "type":"keyword"
                }
             }
          },
          "user":{
             "type":"nested",
             "properties":{
                "id":{
                   "type":"keyword"
                },
                "keyname":{
                   "type":"keyword"
                },
                "display_name":{
                   "type":"keyword"
                }
             }
          }
       }
    },
    "version": 1
 }